{
    "containers": [
        {
            "id": "nginx-main",
            "labels": {
                "app": "nginx"
            },
            "env": [
                "APP_NAME=nginx"
            ],
            "k8s": {
                "namespace": "default",
                "pod": "nginx-pod-1",
                "container_name": "nginx-main",
                "labels": {
                    "app.kubernetes.io/name": "nginx"
                },
                "paused_container": false
            }
        },
        {
            "id": "nginx-sidecar",
            "labels": {
                "app": "nginx"
            },
            "env": [
                "APP_NAME=nginx"
            ],
            "k8s": {
                "namespace": "default",
                "pod": "nginx-pod-1",
                "container_name": "nginx-sidecar",
                "labels": {
                    "app.kubernetes.io/name": "nginx"
                },
                "paused_container": false
            }
        },
        {
            "id": "redis-main",
            "labels": {
                "app": "redis"
            },
            "env": [
                "APP_NAME=redis"
            ],
            "k8s": {
                "namespace": "default",
                "pod": "redis-pod-1",
                "container_name": "redis-main",
                "labels": {
                    "app.kubernetes.io/name": "redis"
                },
                "paused_container": false
            }
        },
        {
            "id": "mysql-init",
            "labels": {
                "app": "mysql"
            },
            "env": [
                "APP_NAME=mysql"
            ],
            "k8s": {
                "namespace": "default",
                "pod": "mysql-pod-1",
                "container_name": "mysql-init",
                "labels": {
                    "app.kubernetes.io/name": "mysql"
                },
                "paused_container": false
            }
        }
    ],
    "test_cases": [
        {
            "name": "k8s_container_exact_match",
            "description": "测试容器名称精确匹配",
            "filters": {
                "K8sContainerRegex": "nginx-main"
            },
            "expected_matched_ids": [
                "nginx-main"
            ]
        },
        {
            "name": "k8s_container_regex_match",
            "description": "测试容器名称正则匹配（如 ^nginx-.*）",
            "filters": {
                "K8sContainerRegex": "^nginx-.*"
            },
            "expected_matched_ids": [
                "nginx-main",
                "nginx-sidecar"
            ]
        },
        {
            "name": "k8s_container_multiple_match",
            "description": "测试多个容器名称的正则匹配",
            "filters": {
                "K8sContainerRegex": ".*-main$"
            },
            "expected_matched_ids": [
                "nginx-main",
                "redis-main"
            ]
        }
    ]
}